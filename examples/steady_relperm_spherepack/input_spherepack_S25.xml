<?xml version="1.0" ?>

<!-- Load the state of a saved simulation -->
<restart>
    <!-- Load the state of a saved simulation -->
    <load_savedstated> false </load_savedstated>
    <!-- Path of the fluid1 save state WITH PATH! -->
    <fluid1_savefile> restart/spheres4Palabos_130000_lattice1.dat </fluid1_savefile>
    <!-- Path of the fluid2 save state WITH PATH!-->
    <fluid2_savefile> restart/spheres4Palabos_130000_lattice2.dat </fluid2_savefile>
    <!-- Restart form run number..  -->
    <restart_run_number> 1 </restart_run_number>
    <!-- Restart time  -->
    <restart_time> 130000 </restart_time>
</restart>

<geometry>
    <!-- The name of the .dat file that with the geometry. -->
    <file_geom> input/spheres4Palabos_200_200_200.dat </file_geom>
    <!-- Size (in voxels) of the simulation domain. -->
    <size> <x> 200 </x> <y> 200 </y> <z> 200 </z> </size>
    <!-- Periodicity in the boundary conditions -->
    <per>
          <fluid1> <x> true </x> <y> true </y> <z> true </z> </fluid1>
          <fluid2> <x> true </x> <y> true </y> <z> true </z> </fluid2>
    </per>
</geometry>

<init>
  <!-- Initial position of fluid 1 -->
    <fluid1>
          <x1> 001 </x1> <y1> 001 </y1> <z1> 001 </z1>
          <x2> 050 </x2> <y2> 200 </y2> <z2> 200 </z2>
    </fluid1>
  <!-- Initial position of fluid 2  -->
    <fluid2>
          <x1> 051 </x1> <y1> 001 </y1> <z1> 001 </z1>
          <x2> 200 </x2> <y2> 200 </y2> <z2> 200 </z2>
    </fluid2>
</init>


<fluids>
    <Gc> 0.9 </Gc>
    <omega_f1> 1 </omega_f1>
    <omega_f2> 1 </omega_f2>
    <force_f1> 0.00001 </force_f1>
    <force_f2> 0.00001 </force_f2>
    <!-- Wetting forces Huang et al., 2007. -->
    <G_ads_f1_s1> -0.4 </G_ads_f1_s1>
    <G_ads_f1_s2>  0.0 </G_ads_f1_s2>
    <G_ads_f1_s3>  0.0 </G_ads_f1_s3>
    <G_ads_f1_s4>  0.0 </G_ads_f1_s4>

	  <!-- Intial density of fluid 1 -->
    <rho_f1> 2 </rho_f1>
    <!-- Initial density of fluid 2 -->
    <rho_f2> 2 </rho_f2>
    <!-- Include a pressure gradient bc (for flow), Use palabos bulidin boundary condition-->
    <bulidin_pressure_bc> false </bulidin_pressure_bc>

    <manual_boundary>
        <!-- Achieve density boundary condition by change density at boundary-->
        <use_manual_boundary> True </use_manual_boundary>
        <!-- Frequence of update density at boundary-->
        <update_frequence> 10 </update_frequence>
    </manual_boundary>
    <!-- position of inlet boundary-->
    <inlet_position>
            <!-- If bulidin_pressure_bc is chosen, the boundary should be 1 layer thick (x1=x2)-->
            <x1> 0 </x1> <y1> 0 </y1> <z1> 0 </z1>
            <x2> 1 </x2> <y2> 200 </y2> <z2> 200 </z2>
    </inlet_position>
    <!-- position of outlet boundary-->
    <outlet_position>
            <!-- If bulidin_pressure_bc is chosen, the boundary should be 1 layer thick (x1=x2)-->
            <x1> 199 </x1> <y1> 0 </y1> <z1> 0 </z1>
            <x2> 200 </x2> <y2> 200 </y2> <z2> 200 </z2>
    </outlet_position>
    <!-- Density of fluid 1 at the inlet (Constant)-->
    <rho_f1_i> 2 </rho_f1_i>
    <!-- Initial Density of fluid 2 at the outlet -->
    <rho_f2_i> 2 </rho_f2_i>
    <!-- Final density of fluid 2 at the outlet (after n-steps) -->
    <rho_f2_f> 2 </rho_f2_f>
    <!-- Dissolved density of one phase in the other (both f1 and f2) -->
    <rho_d> 0.06 </rho_d>
    <!-- Decrement in outlet density (equivalent to pressure change) per cap increment -->
    <drho_f2> 0.1 </drho_f2>
</fluids>


<output>
    <!-- Output folder -->
    <out_folder> tmp_25/ </out_folder>
    <!-- Convergence criteron (density change) -->
    <convergence> 0.001 </convergence>
    <!-- Max iterations per Pc step -->
    <it_max> 1000000 </it_max>
    <!-- Check for convergence every -->
    <it_conv> 1000 </it_conv>
    <!-- Print density gifs -->
    <it_gif> 1000 </it_gif>
    <!-- Write f1 and f2 density vtks (True). Only f1 (False) -->
    <rho_vtk> False </rho_vtk>
    <!-- print vtks every (big files) -->
    <it_vtk> 1000 </it_vtk>
    <!-- Save the simulation state after every capillary increment (big files) -->
    <save_sim> True </save_sim>
    <!-- The name of the .dat file that Save the simulation state -->
    <save_name> pheres4Palabos </save_name>
    <!-- Save state every.. -->
    <it_save> 10000 </it_save>
    <!-- print geometry at the beginning -->
    <print_geom> True </print_geom>
    <!-- print stl (for 3D printing and viewing)  -->
    <print_stl> True </print_stl>
</output>
